<!DOCTYPE html>
<html>
<head>
  <title>TTS</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:notice] %>
    <div id="notice"><%= flash[:notice] %></div>
<% end %>

<% if flash[:error] %>
    <div id="error"><%= flash[:error] %></div>
<% end %>
<%= yield %>

</body>
</html>
